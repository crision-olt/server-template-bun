{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --concurrency 11",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\" --ignore-unknown",
    "lint:spelling": "cspell \"**\" \".github/**/*\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@turbo/gen": "^1.10.7",
    "@types/node": "^20.4.2",
    "cspell": "^6.31.1",
    "eslint": "^8.44.0",
    "tsconfig": "workspace:*",
    "eslint-config-custom": "workspace:*",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "turbo": "^1.10.7",
    "typescript": "^5.1.6"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write"
  },
  "packageManager": "pnpm@7.15.0",
  "name": "template-bun",
  "author": "Cristian Ionut Olteanu <crision.olt@gmail.com>"
}
